{
  "name": "hmlr-frontend",
  "version": "0.1.0",
  "description": "HMLR frontend styles",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prettier": "prettier --write '{src,.storybook}/**/*.{js,scss,mdx}'",
    "build": "npm run lint && npm run styles",
    "lint": "prettier --check '{src,.storybook}/**/*.{js,scss,mdx}'",
    "styles": "sass --style=compressed --load-path=node_modules src/hmlr/all.scss dist/hmlr-frontend.scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LandRegistry/hmlr-frontend.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LandRegistry/hmlr-frontend/issues"
  },
  "homepage": "https://github.com/LandRegistry/hmlr-frontend#readme",
  "dependencies": {
    "govuk-frontend": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@mdx-js/react": "^2.1.2",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/html": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "simple-nunjucks-loader": "^3.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
